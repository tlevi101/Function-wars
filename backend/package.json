        "bcryptjs": "^2.4.3",
        "chalk": "^4.1.2",
        "crypto-js": "^4.1.1",
        "date-and-time": "^2.3.1",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "express-jwt": "^7.7.2",
        "http-status-codes": "^2.2.0",
        "indent-string": "^4.0.0",
        "jest": "^29.4.2",
        "joi": "^17.9.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "lz-string": "^1.4.4",
        "mysql2": "^3.1.0",
        "nodemailer": "^6.9.1",
        "sequelize": "^6.19.0",
        "slug": "^5.3.0",
        "socket.io": "^4.6.1",
        "socketio-jwt": "^4.6.2",
        "supertest": "^6.3.3",
        "table": "^6.8.0",
        "uuid": "^8.3.2"
